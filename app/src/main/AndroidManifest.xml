<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.POST_NOTIFICATIONS" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.B07_Project"
        tools:targetApi="31">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.B07_Project">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddFeedbackActivity" />
        <activity android:name=".EventActivity"/>
        <activity android:name=".FormActivity" />
        <activity android:name=".FeedbackActivity"/>
        <activity android:name=".ComplaintActivity" />
        <activity android:name=".AnnouncementsListActivity" />
        <activity android:name=".PostAnnouncementsActivity" />
        <activity android:name=".ReceiveComplaintActivity" />
        <activity android:name=".HomeActivity"/>
        <activity android:name=".PostreqActivity"/>
        <activity android:name=".NotificationsActivity"/>
        <activity android:name=".RegisterActivity"/>
    </application>

</manifest>